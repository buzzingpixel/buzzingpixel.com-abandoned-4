.HeadingBackground {
    border-bottom: 6px solid var(--DeepSea);
    line-height: 0;
    height: 200px;
    left: 0;
    overflow: hidden;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 1;
}

.HeadingBackground__Img {
    height: 100%;
    object-fit: cover;
    object-position: 50% 50%;
    width: 100%;
}
