{% do requireVariables({
    license: 'App\\Licenses\\Models\\LicenseModel',
}) %}

{# @var \App\Licenses\Models\LicenseModel license #}

{% extends 'Account/_AccountLayout.twig' %}

{% block accountContent %}
    {% embed 'StandardForm/StandardFormEmbed.twig' with {
        heading: 'Edit Notes on<br>' ~ license.id,
        submitButtonContent: 'Submit',
        license: license,
    } only %}
        {% block standardFormContent %}
            {% import "StandardForm/Macros/StandardFormInputs.twig" as inputs %}
            {{ inputs.textArea(
                'Notes',
                'notes',
                license.notes,
                message
            ) }}
        {% endblock %}
    {% endembed %}
{% endblock %}
